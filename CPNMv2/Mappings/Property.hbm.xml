<?xml version="1.0" encoding="utf-8" ?>

<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="CPNMv2" namespace="CPNMv2.Domain">
  <class name="Property">
    <id name="Id">
      <generator class="guid"/>
    </id>
    <property name="Name"/>
    <property name="Description"/>
    <property name="IsCalculated"/>
    <many-to-one name="DefaultUnit" cascade="save-update"/>
    <many-to-one name="Dimension" cascade="all" not-null="false" lazy="false"/>
    <many-to-one name="PropertyGroup" cascade="save-update" not-null="false"/>
  </class>
</hibernate-mapping>
