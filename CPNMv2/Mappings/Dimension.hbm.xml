<?xml version="1.0" encoding="utf-8" ?>

<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="CPNMv2" namespace="CPNMv2.Domain">
  <class name="Dimension">
    <id name="Id">
      <generator class="guid"/>
    </id>
    <property name="Name"/>
    <set name="Units" cascade ="all-delete-orphan" lazy="false">
      <key column = "Dimension"/>
      <one-to-many class="UnitOfMeasure"/>
    </set>
  </class>
</hibernate-mapping>
